{
  "dependencies": {
    "axios": "^1.3.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "csv-stringify": "^6.5.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "express-session": "^1.18.2",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "*.js",
      "!app.js",
      "!jest.config.js",
      "!coverage/**",
      "!tests/**"
    ],
    "coverageDirectory": "coverage",
    "testTimeout": 30000
  }
}
